
CC=cicc -I.

all: qt stp testpgm

qt:
	ln -s md/setjmp_d.h qtmd.h
	$(CC) -c -o qt.o qt.c
	$(CC) -c -o qtmds.o md/setjmp.c
	xar qs libqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	xar qs libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	$(CC) -c -o qtmdb.o md/setjmp_b.c
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
