
CC=cc -I. -O

all: qt stp testpgm

qt:
	rm -f qtmd.h
	ln -s md/sparc.h qtmd.h
	$(CC) -c -o qt.o qt.c
	$(CC) -c -o qtmds.o md/sparc.s
	ar crv libqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar crv libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	$(CC) -c -o qtmdb.o md/sparc_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
	
