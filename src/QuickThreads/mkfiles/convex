
CC=/bin/cc -I. -O

all: qt stp testpgm

qt:
	rm -f qtmd.h
	ln -s md/hppa.h qtmd.h
	$(CC) -pedantic -Wall -c -o qt.o qt.c
	cc -c -o qtmds.o md/hppa.s
	ar q libqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	cc -c -o qtmdb.o md/hppa_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
