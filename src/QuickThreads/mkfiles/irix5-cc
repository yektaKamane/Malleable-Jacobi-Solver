
CC=cc -n32 -I. -O
AS=as -n32

all: qt stp testpgm

qt:
	rm -f qtmd.h
	ln -s md/mips.h qtmd.h
	$(CC) -c -o qt.o qt.c
	$(AS) -o qtmds.o md/mips-irix5.s
	ar q libqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	$(AS) -o qtmdb.o md/mips_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
