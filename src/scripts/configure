protected:
        int len;
};
class foo: public foo_foo {
public:
	template<func_t f> //Templated member function
	void memb(void) {f(); len++;}
};
inline void fn() {}
void myfunc()
{
  foo x;
  x.template memb<fn>();
}
EOT
